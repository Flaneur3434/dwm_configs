# http://xahlee.info/linux/linux_show_keycode_keysym.html
#rofi
super + o
  ~/Documents/scripts/rofi_dwm.sh

#screenshot
ctrl + shift + g
  	gscreenshot

#Caprine
ctrl + shift + m
  	caprine

#Discord
ctrl + shift +d
  	discord

#Volume Up
ctrl + shift + a
  	pactl set-sink-volume 0 +5%

XF86AudioRaiseVolume
	pactl set-sink-volume 0 +5%
	
#Volume Down
ctrl + shift + s
  	pactl set-sink-volume 0 -5%

XF86AudioLowerVolume
	pactl set-sink-volume 0 -5%

#MuteVol
XF86AudioMute
	pactl set-sink-mute 0 toggle

#MuteMic
XF86AudioMicMute
	pactl set-source-mute @DEFAULT_SOURCE@ toggle

#Brightness
#use xbindkeys --key to figure out keys
XF86MonBrightnessDown
  	xbacklight -dec 10

XF86MonBrightnessUp
  	xbacklight -inc 10

alt + Tab
    skippy-xd-runner --activate 

ctrl + shift + z
	91plumber

#terminal
super + Return
	urxvt

#browser
super + b
	librewolf

#Flameshot
Print
	flameshot gui
